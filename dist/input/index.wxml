


  <view>

   <view class='many-input-fresh'>
    <view class="per-input-fresh">
      <text class='text-fresh'>店铺名称</text>
      <input class='input-fresh' bindinput='inputeBayStoreName' placeholder="请输入店铺名称"></input>
    </view>

    <view class="per-input-fresh">
      <text class='text-fresh'>eBay邮箱</text>
      <input class='input-fresh' bindinput='inputeBayEmail' placeholder="请输入eBay登陆邮箱"></input>
    </view>

    <view class="per-input-fresh">
      <text class='text-fresh'>店铺地址（URL）</text>
      <input class='input-fresh' bindinput='inputeBayWebSiteAddress' placeholder="请输入店铺网站首页地址"></input>
    </view>

    <view class="per-input-fresh">
      <text class='text-fresh'>PayPal账户邮箱</text>
      <input class='input-fresh' bindinput='inputeBayPayPalEmail' placeholder="请输入PayPal账户邮箱"></input>
    </view>

    <view class="per-input-fresh">
      <text class='text-fresh'>PayPal商家信息</text>
      <input class='input-fresh' bindinput='inputeBayPayPalMerInfo' placeholder="请输入PayPal商家信息"></input>
    </view>

    <view class="per-input-fresh">
      <text class='text-fresh'>PayPal商家账号</text>
      <input class='input-fresh' bindinput='inputeBayPayPalMerAccount' placeholder="请输入PayPal商家账号"></input>
    </view>
   </view>
  </view>
<view class='button-style'></view>

<button catchtap='submitInfo'>提交信息</button>

