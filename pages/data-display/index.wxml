<view class='small-title'>数据显示</view>

<view class='layout-style'>
  <view class='text-style1'>
    <text class='per-content'>店铺名称</text>
    <text class='per-content'>所属平台</text>
    <text class='per-content'>到账金额</text>
    <text class='per-content'>国际币种</text>
    <text class='per-content'>到账时间</text>
  </view>
  <block wx:for="{{components}}" wx:key="waihui"> 
    <view class='content-style'>
      <text class='per-content'>{{item.First}}</text>
      <text class='per-content'>{{item.Second}}</text>
      <text class='per-content'>{{item.Third}}</text>
      <text class='per-content'>{{item.Fourth}}</text>
      <text class='per-content'>{{item.Fifth}}</text>
    </view>
  </block>
</view>

<view class='small-title'>列表显示</view>

<view class="info-per">
  <text class='text-style2'>姓名(中文)</text>
  <input class='input-style' disabled='true' value='{{GetInfo.CertificationInfo.UserName}}' ></input>
</view>



  